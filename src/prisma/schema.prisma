generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

enum UserRole {
  SUPER_ADMIN
  VENDOR
  VENDOR_USER
}

model User {
  id          Int     @id @default(autoincrement())
  name        String
  email       String
  role        UserRole   @default(VENDOR)
  createdAt   DateTime   @default(now())
  updatedAt   DateTime   @updatedAt
}
